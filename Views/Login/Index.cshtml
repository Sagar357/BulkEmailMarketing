                        <a href="#">Forgot your password?</a>
                        <a href="#" class="Remember_ME">
                            <input type="checkbox" name="Remember" id="Remember">
                            Remember me
                        </a>
                        <button>
                            <i class="spinner"></i>
                            <span class="state">Log in</span>
                        </button>
                    </form>
                    </p>
                </div>
            </div>
        </div>
    </div>


    <!-- scritp section -->

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
