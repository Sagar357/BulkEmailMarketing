
@{
    ViewBag.Title = "Login";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lg</title>
    <link rel="stylesheet" href="~/Content/Style.css">
    <link rel="stylesheet" href="~/Scripts/fontawesome-free-5.15.1-web/css/all.css">
</head>
<body>
    <div class="site_container">
        <div class="container">
            <div class="main_Wrp">
                <div class="wrapper">
                    <div class="Lg_frst L_cmn01">
                        <figure>
                            <img src="~/img/undraw_mobile_login_ikmv.png" alt="login">
